{
   "/api/*": {
      "target":  {
        "host": "apps.jw.org",
        "protocol": "https:",
        "port": 443
      },
      "secure": false,
       "logLevel": "debug",
       "pathRewrite": {"^/api" : "https://apps.jw.org/api/public/meeting-search"},
      "changeOrigin": true
   },
   "/wol/*": {
      "target":  {
        "host": "wol.jw.org",
        "protocol": "https:",
        "port": 443
      },
      "secure": false,
       "logLevel": "debug",
       "pathRewrite": {"^/wol" : "https://wol.jw.org/wol/"},
      "changeOrigin": true
   }
 }
